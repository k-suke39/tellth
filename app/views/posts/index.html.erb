		<section class="l-content-fixed section">
			<header class="section-header">
				<h2 class="section-title">Question</h2>
				<p class="section-subtitle">質問一覧</p>
			</header>
			<table class="table-list">
				<colgroup>
					<col class="table-list-col-type">
					<col class="table-list-col-date">
					<col class="table-list-col-data">
                    <col class="table-list-col-data">
				</colgroup>
				<tbody>
                    <% @posts.each do |post|%>
					<tr>
						<td><span class="table-list-icon">未解決</span></td>
						<td class="table-list-date"><time datetime="2018-09-13">2018.09.13</time></td>
						<td class="table-list-link"><a href="20180913.html"><%= link_to(post.title, post)%></a></td>
						<% if @current_user.id == post.user_id%>
                          <td><span class="table-list-status-edit"><%= link_to("編集",edit_post_path(post))%></span></td>
						  <td><span class="table-list-status-delete"><%= link_to("削除", post_path(post), data: {"turbo-method": :delete})%></span></td>
						<% else %>
						  <td><span class=""></span></td>
						  <td><span class=""></span></td>
						<% end %>
					</tr>
                     <% end %>
				</tbody>
			</table>
		</section>